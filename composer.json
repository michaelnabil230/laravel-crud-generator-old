{
    "name": "michaelnabil230/laravel-crud-generator",
    "description": "This is my package laravel-crud-generator",
    "keywords": [
        "MichaelNabil230",
        "laravel",
        "laravel-crud-generator"
    ],
    "homepage": "https://github.com/michaelnabil230/laravel-crud-generator",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Nabil",
            "email": "46572405+michaelnabil230@users.noreply.github.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^9.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "MichaelNabil230\\LaravelCrudGenerator\\": "src",
            "MichaelNabil230\\LaravelCrudGenerator\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MichaelNabil230\\LaravelCrudGenerator\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MichaelNabil230\\LaravelCrudGenerator\\LaravelCrudGeneratorServiceProvider"
            ],
            "aliases": {
                "LaravelCrudGenerator": "MichaelNabil230\\LaravelCrudGenerator\\Facades\\LaravelCrudGenerator"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
